{
  "name": "use-slots",
  "version": "1.0.2",
  "description": "Slot placement for React",
  "main": "dist/index.js",
  "module": "dist/use-slots.esm.js",
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "size-limit": [
    {
      "path": "dist/use-slots.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/use-slots.esm.js",
      "limit": "10 KB"
    }
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "prerelease": "npm run build",
    "release": "release-it"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "react-hooks",
    "hooks",
    "react",
    "utils",
    "slot",
    "slots"
  ],
  "author": "Hyan Mandian",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^2.4.2",
    "@size-limit/preset-small-lib": "^4.12.0",
    "@testing-library/react-hooks": "^7.0.0",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.11",
    "husky": "^6.0.0",
    "react": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "release-it": "^14.10.0",
    "size-limit": "^4.12.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.4"
  },
  "dependencies": {
    "react-keyed-flatten-children": "^1.3.0"
  },
  "release-it": {
    "github": {
      "release": true
    }
  }
}
